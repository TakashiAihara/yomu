# Copy this file to secret.yaml and fill in your actual values
# kubectl create secret generic yomu-secrets --from-literal=... -n yomu

apiVersion: v1
kind: Secret
metadata:
  name: yomu-secrets
  namespace: yomu
type: Opaque
stringData:
  DATABASE_URL: "postgresql://yomu:yomu@postgres:5432/yomu"
  VALKEY_URL: "redis://valkey:6379"
  GOOGLE_CLIENT_ID: "your-google-client-id"
  GOOGLE_CLIENT_SECRET: "your-google-client-secret"
  GOOGLE_REDIRECT_URI: "http://localhost:8085/callback"
  SESSION_SECRET: "your-session-secret-min-32-chars"
